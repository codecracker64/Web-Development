<%- include('partials/header.ejs') %>
  <div class="container">
    <div class="container-wrap">
      <h1>Home</h1>
      <p>
        <%= home %>
      </p>
    </div>
      <div class="posts">
      <% posts.forEach(function(posts){ %>
        <h2><%= posts.title %></h2>
        <p><%= posts.body.substring(0,100) +"..." %><a href="/posts/<%= posts._id%>">Read more</a></p>
      <% }) %>
    </div>
  </div>
<%- include('partials/footer.ejs') %>
